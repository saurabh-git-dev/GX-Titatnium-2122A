{
    "manifest_version": 3,
    "name": "My Script Injector",
    "version": "1.0",
    "description": "Injects custom JavaScript.",
    "permissions": ["scripting"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ]
  }
  